{
    "name": "manyou/mango",
    "description": "Useful components for Symfony development",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "symfony/dependency-injection": "^6.3",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/doctrine-bundle": "^2.7",
        "symfony/framework-bundle": "^6.3",
        "symfony/uid": "^6.3",
        "symfony/monolog-bundle": "^3.8",
        "doctrine/dbal": "^3.5.1",
        "doctrine/migrations": "^3.2",
        "psr/log": "^2.0 || ^3.0",
        "monolog/monolog": "^3.0",
        "symfony/messenger": "^6.3",
        "symfony/scheduler": "^6.3"
    },
    "require-dev": {
        "symfony/http-kernel": "^6.3",
        "symfony/yaml": "^6.3",
        "moneyphp/money": "^4.1",
        "ext-intl": "*",
        "symfony/lock": "^6.3",
        "symfony/cache": "^6.3",
        "symfony/serializer": "^6.3",
        "symfony/validator": "^6.3"
    },
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Manyou\\Mango\\": ""
        },
        "exclude-from-classmap": [
          "/Tests/"
        ]
    },
    "authors": [
        {
            "name": "Brent, Bohan Yang",
            "email": "brent@manyou.io"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    }
}
